	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
//			"program": "${workspaceFolder}/test_algo2",
			"program": "${workspaceFolder}/push_swap_debug",
//			"args": ["6", "5","12", "-4", "-9", "7", "-5", "3", "-11", "-3", "4", "9", "-10", "-12", "2", "11", "-1", "-7", "-8", "10", "-6", "1", "-2", "0", "8"],
			"args": ["2", "3", "1", "4"],
//			"args": [],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}

	]
}
