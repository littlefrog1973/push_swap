	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
//			"program": "${workspaceFolder}/checker_bonus",
//			"program": "${workspaceFolder}/test_algo2",
			"program": "${workspaceFolder}/push_swap",
			"args": ["6", "4", "5", "2", "1", "3", "7", "0", "10"],
//			"args": ["5", "7", "1", "8", "2", "4", "3", "9", "10", "6"],
//			"args": ["10", "9","8", "7", "6", "-1", "1", "2", "3", "4", "-10", "0", "-20", "12", "30", "-2"],
//			"args": ["5", "7","14", "3", "11", "8", "10", "2", "12", "6", "13", "1", "9", "0", "4"],
//			"args": ["2", "3", "1", "4"],
//			"args": [" 4 "," 2", "5"],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}

	]
}
